class pascalTrianglePattern   
{   
public static void main(String args[])   
{   
int i, j, row=6;   
for(i=5; i>=0; i--)   
{   
for (j=2*(row-i); j>=0; j--)         
{  
System.out.print(" ");   
}   
for(j=0; j<=i; j++)   
{   
System.out.print("* ");   
}   
System.out.println();   
}   
}   
}  
